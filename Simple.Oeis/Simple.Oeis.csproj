<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>Simple.Oeis</Title>
    <Authors>Dmitrii Bychenko</Authors>
    <Description>OEIS (Online Encyclopedia of Integer Sequences) queries</Description>
    <Copyright>Copyright (c) 2025 by Dmitrii Bychenko</Copyright>
    <PackageReadmeFile>Simple.Oeis.md</PackageReadmeFile>
    <PackageIcon></PackageIcon>
    <PackageReleaseNotes>First release of OEIS Encyclopedia API</PackageReleaseNotes>
	<PackageIcon>Icon.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Http" Version="9.0.9" />
  </ItemGroup>

  <ItemGroup>
    <None Update="Metadata\Icon.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
	<None Include="Metadata\Icon.png" Pack="true" PackagePath="" />
	<None Include="Simple.Oeis.md" Pack="true" PackagePath="" />
  </ItemGroup>

</Project>
